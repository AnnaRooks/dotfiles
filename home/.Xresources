! special
*.foreground:   #d1d1d1
! *.background:   #06060a
*.cursorColor:  #d1d1d1

! 8 normal colors
*.color0:       #272822
*.color1:       #f92672
*.color2:       #a6e22e
*.color3:       #f4bf75
*.color4:       #66d9ef
*.color5:       #ae81ff
*.color6:       #a1efe4
*.color7:       #f8f8f2
! 8 bright colors
*.color8:       #75715e
*.color9:       #f92672
*.color10:      #a6e22e
*.color11:      #f4bf75
*.color12:      #66d9ef
*.color13:      #ae81ff
*.color14:      #a1efe4
*.color15:      #f9f8f5

URxvt.font: xft:DroidSansMonoForPowerline:size=10, \
            xft:TwitterColorEmojiSVGinOT:size=10, \
            xft:FontAwesome5FreeSolid:size=5, \
            xft:FontAwesome5Brands:size=5, \
            xft:DejaVuSansMono:size=10
URxvt.letterSpace: 0

URxvt.perl-ext-common: bell-command,matcher,font-size
URxvt.bell-command: aplay $HOME/.config/glass.wav
URxvt.urgentOnBell: true
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

URxvt.scrollBar: false

URxvt.depth: 32
URxvt.background: [80]#06060a
